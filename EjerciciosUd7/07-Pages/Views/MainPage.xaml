<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="_07_Pages.MainPage">
    <StackLayout>
        <Entry Margin="10"
               WidthRequest="200"
               HorizontalOptions="Start"
               Placeholder="Introduce el nombre"
               x:Name="NombreUsu"/>

        <Entry Margin="10"
               WidthRequest="200"
               HorizontalOptions="Start"
               Placeholder="Introduce los apellidos"
               x:Name="ApellidosUsu"/>

        <Button Text="Ir a Pagina Tabbed"
                WidthRequest="200"
                Margin="10"
                HorizontalOptions="Start"
                x:Name="navegarTabbed"
                Clicked="irPaginaTabbed"/>

        <Button Text="Página 4"
                WidthRequest="200"
                HorizontalOptions="Start"
                Margin="10"
                Clicked="irPage4"/>

        <Button Text="Página 5"
                WidthRequest="200"
                HorizontalOptions="Start"
                Margin="10"
                Clicked="irPage5"/>
    </StackLayout>

</ContentPage>
